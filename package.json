{
  "name": "@human-writes/vue-plugin",
  "description": "Writes HTML and code snippet like a human",
  "version": "0.5.61",
  "main": "./dist/VueWriterPlugin.umd.cjs",
  "module": "./dist/VueWriterPlugin.js",
  "exports": {
    ".": {
      "import": "./dist/VueWriterPlugin.js",
      "require": "./dist/VueWriterPlugin.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "types": "vue-tsc ",
    "preview": "vite preview",
    "roll": "rollup -c",
    "lint": "npx eslint --ext .mjs,.cjs,.js ./src",
    "test": "echo \"Nothing to test right now\" && exit 0"
  },
  "type": "module",
  "keywords": [
    "HTML",
    "code-snippet",
    "human",
    "writing"
  ],
  "author": "David Blanchard",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/human-writes/vue-plugin/issues"
  },
  "homepage": "https://github.com/human-writes/vue-plugin#readme",
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "rollup": "^3.25.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vue-tsc": "^1.8.1"
  },
  "dependencies": {
    "@human-writes/core": "^0.5.63"
  }
